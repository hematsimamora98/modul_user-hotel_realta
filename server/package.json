        "express-boom": "^3.0.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.5",
        "nodemon": "^3.0.1",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
